<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
    xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
    xmlns:ttywidgets='urn:import:com.google.testing.testify.risk.frontend.client.view.widgets'>

  <!-- Capability -->
  <gwt:HorizontalPanel ui:styleName="tty-Capability">
    <gwt:Cell width="16">
      <gwt:Image ui:styleName="tty-CapabilityGripper" ui:field="capabilityGripper" visible="false" />
    </gwt:Cell>
    <gwt:VerticalPanel ui:styleName="tty-CapabilityContent">
        <ttywidgets:EasyDisclosurePanel ui:field="disclosurePanel">
          <gwt:SimplePanel ui:field="disclosureContent" />
        </ttywidgets:EasyDisclosurePanel>
    </gwt:VerticalPanel>
    <gwt:Cell width="20">
      <gwt:Image ui:field="deleteCapabilityImage" ui:styleName="tty-CapabilityDeleteImage" visible="false" ui:url="images/x.png" />
    </gwt:Cell>
    <gwt:Cell width="0">
      <gwt:Label ui:field="capabilityId" visible="false" />
    </gwt:Cell>
    
  </gwt:HorizontalPanel>

</ui:UiBinder>
