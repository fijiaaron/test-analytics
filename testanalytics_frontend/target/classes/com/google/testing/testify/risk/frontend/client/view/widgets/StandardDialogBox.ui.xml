<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:gwt='urn:import:com.google.gwt.user.client.ui'>

  <!-- Standard Dialog Box -->
  <gwt:VerticalPanel>
    <gwt:VerticalPanel ui:field="dialogContent"></gwt:VerticalPanel>

    <gwt:HorizontalPanel ui:styleName="tty-StandardDialogBoxButtonPanel">
      <gwt:Button ui:field="okButton">OK</gwt:Button>
      <gwt:Button ui:field="cancelButton">Cancel</gwt:Button>
    </gwt:HorizontalPanel>
    
  </gwt:VerticalPanel>
  
</ui:UiBinder>